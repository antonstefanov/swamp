  ],
  "devDependencies": {
    "bs-platform": "^4.0.14",
    "rollup": "^1.0.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0"
  },
  "dependencies": {
    "den-carbon-core": "^0.0.3",
    "den-crystal": "^0.0.3",
    "den-seed": "^0.0.3",
    "yargs": "^12.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antonstefanov/swamp"
  },
  "author": "Anton Stefanov"
