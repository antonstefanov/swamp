{
  "name": "den-carbon",
  "private": false,
  "version": "0.0.3",
  "license": "MIT",
  "bin": {
    "o": "dist/carbon"
  },
  "scripts": {
    "install-carbon-dev": "npm link",
    "build": "bsb -make-world",
